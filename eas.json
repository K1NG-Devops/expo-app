{
  "cli": {
    "version": ">= 5.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "dev",
        "EAS_PROJECT_ID": "4926d822-0e8c-49eb-b1b0-c6adf9285cbd",
        "EXPO_PUBLIC_ENABLE_TEST_TOOLS": "1",
        "EXPO_PUBLIC_ENABLE_CONSOLE": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "credentialsSource": "remote",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_SUPABASE_URL": "https://your-project.supabase.co",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your_new_anon_key_here",
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_PUBLIC_TENANT_SLUG": "preview",
        "EXPO_PUBLIC_WHATSAPP_API_ENABLED": "true",
        "EXPO_PUBLIC_SCHOOL_WHATSAPP_NUMBER": "+27674770975",
        "EXPO_PUBLIC_API_BASE": "https://lvvvjywrmpcqrpvuptdi.supabase.co/functions/v1",
        "EXPO_PUBLIC_ENABLE_TEST_ADS": "true",
        "EXPO_PUBLIC_ENABLE_ADS": "1",
        "EXPO_PUBLIC_ENABLE_TEST_TOOLS": "1",
        "EXPO_PUBLIC_ENABLE_OTA_UPDATES": "true"
      }
    },
    "production": {
      "channel": "production",
      "credentialsSource": "remote",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "prod",
        "NODE_ENV": "production",
        "EXPO_PUBLIC_SUPABASE_URL": "https://your-project.supabase.co",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your_new_anon_key_here",
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_TENANT_SLUG": "production",
        "EXPO_PUBLIC_WHATSAPP_API_ENABLED": "true",
        "EXPO_PUBLIC_SCHOOL_WHATSAPP_NUMBER": "+27674770975",
        "EXPO_PUBLIC_API_BASE": "https://lvvvjywrmpcqrpvuptdi.supabase.co/functions/v1",
        "EXPO_PUBLIC_ENABLE_CONSOLE": "false",
        "EXPO_PUBLIC_ENABLE_ADS": "1",
        "EXPO_PUBLIC_ENABLE_TEST_ADS": "false",
        "EXPO_PUBLIC_ENABLE_MOCK_ADS": "false",
        "EXPO_PUBLIC_ENABLE_TEST_TOOLS": "0",
        "EXPO_PUBLIC_ENABLE_OTA_UPDATES": "true"
      }
    },
    "production-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
